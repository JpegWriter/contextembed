{
  "planVersion": "1.0.0",
  "product": {
    "name": "ContextEmbed",
    "positioning": {
      "oneLiner": "Context onboarding + vision analysis + standards-aware embedding so files stay self-describing.",
      "corePromise": "Embed meaning into the file, not just into a database.",
      "differentiators": [
        "Context-first onboarding (brand, rights, intent) before processing images",
        "Vision analysis + user context synthesis (not generic auto-tagging)",
        "Standards-aware mapping to IPTC/EXIF/XMP with verification read-back",
        "Audit trail for every embedded asset (inputs, versions, fields written)"
      ],
      "guardrails": [
        "No ranking guarantees",
        "No claims of inventing IPTC/EXIF",
        "Avoid adversarial language about other platforms",
        "Mention Adobe sparingly and use-case driven"
      ]
    }
  },
